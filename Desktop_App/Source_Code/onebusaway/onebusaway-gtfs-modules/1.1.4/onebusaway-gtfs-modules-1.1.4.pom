<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.onebusaway</groupId>
    <artifactId>onebusaway-gtfs-modules</artifactId>
    <version>1.1.4</version>
    <packaging>pom</packaging>

    <name>onebusaway-gtfs-modules</name>
    <description>Parent bundle of all OneBusAway GTFS modules</description>
    <url>http://code.google.com/p/onebusaway/wiki/ModuleGtfs</url>

    <parent>
        <groupId>org.onebusaway</groupId>
        <artifactId>onebusaway</artifactId>
        <version>1.0.6</version>
        <relativePath>../onebusaway</relativePath>
    </parent>

    <scm>
        <connection>scm:svn:http://onebusaway.googlecode.com/svn/onebusaway-gtfs-modules/tags/onebusaway-gtfs-modules-1.1.4</connection>
        <developerConnection>scm:svn:https://onebusaway.googlecode.com/svn/onebusaway-gtfs-modules/tags/onebusaway-gtfs-modules-1.1.4</developerConnection>
        <url>http://code.google.com/p/onebusaway/source/browse/#svn/onebusaway-gtfs-modules/tags/onebusaway-gtfs-modules-1.1.4</url>
    </scm>

    <modules>
        <module>onebusaway-gtfs</module>
        <module>onebusaway-gtfs-hibernate</module>
        <module>onebusaway-gtfs-transformer</module>
        <module>onebusaway-gtfs-transformer-cli</module>
    </modules>

    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-deploy-plugin</artifactId>
                    <version>2.5</version>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

</project>
